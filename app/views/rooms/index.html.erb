<header class="header">
    <%= link_to image_tag("monbo1.png", class:"monbo"), "#" %>
    <h1>チャット<span>一覧</span></h1>
</header>  
<div class="main">
        <%= render  "share/side_bar" %>

        <div class="mainArea">
            <ul class="chatArea">
           <% @users.each do |user| %>
              <% unless user == current_user || !user.liked_by?(current_user)%>
                <li>
                  <%= link_to user_match_path(id:user, user_id: user.id) do %>  
                    <%= image_tag user.icon.image_path, class: "chatIcon"%>
                  <% end %>
                   <%= user.nick_name%>
                </li>
              <% end %>
           <% end %>
          </ul>
          
        </div>

</div>


